{
  "name": "@kzkymur/storage",
  "version": "1.1.4",
  "module": "./dist",
  "description": "The Super lightweight library to handle WebStorage more easily.",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./react": {
      "import": "./dist/react.js",
      "require": "./dist/react.js",
      "types": "./dist/react.d.ts"
    }
  },
  "scripts": {
    "dev": "webpack --watch --mode development",
    "build": "webpack --mode production",
    "test": "jest ./test",
    "test:src": "TEST_MODE=src jest ./test",
    "release": "npm publish --access=public ./"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kzkymur/storage.js.git"
  },
  "author": "kzkymur",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kzkymur/storage.js/issues"
  },
  "homepage": "https://github.com/kzkymur/storage.js#readme",
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/node": "^20.12.8",
    "@types/react": "^18.3.2",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "react": "^18.3.1",
    "ts-jest": "^29.1.2",
    "ts-loader": "^9.5.1",
    "typescript": "^5.4.5",
    "webpack": "^5.91.0",
    "webpack-cli": "^5.1.4"
  },
  "peerDependencies": {
    "react": "^17.0.0 || ^18.0.0"
  },
  "keywords": [
    "storage",
    "localStorage",
    "sessionStorage",
    "WebStorage",
    "eventlistener",
    "react",
    "hooks"
  ]
}
